{
  "name": "acr-automotive",
  "version": "1.0.0",
  "scripts": {
    "// See SCRIPTS.md for full documentation": "",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "tsx scripts/test/run-all-tests.ts",
    "//": "",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:reset": "tsx scripts/db/reset-database.ts",
    "supabase:status": "npx supabase status",
    "// ": "",
    "db:save-snapshot": "tsx scripts/db/save-dev-snapshot.ts",
    "db:restore-snapshot": "tsx scripts/db/restore-dev-snapshot.ts",
    "db:import-seed": "tsx scripts/db/import-seed-sql.ts",
    "//  ": "",
    "staging:export": "cross-env NODE_ENV=staging tsx scripts/test/export-seed-snapshot.ts",
    "staging:import": "cross-env NODE_ENV=staging tsx scripts/test/export-seed-snapshot.ts --import",
    "staging:clear": "cross-env NODE_ENV=staging tsx scripts/test/delete-all-data.ts",
    "staging:import-seed": "cross-env NODE_ENV=staging tsx scripts/db/import-seed-staging.ts",
    "test:generate-baseline": "cross-env NODE_ENV=test tsx scripts/test/generate-baseline.ts",
    "//   ": "",
    "bootstrap": "cross-env NODE_ENV=production tsx scripts/db/bootstrap-import.ts",
    "bootstrap:test": "cross-env NODE_ENV=test tsx scripts/db/bootstrap-import.ts",
    "check-prod": "cross-env NODE_ENV=production tsx scripts/db/check-production.ts",
    "clear-prod": "cross-env NODE_ENV=production tsx scripts/db/clear-production.ts",
    "prod:import-seed": "cross-env NODE_ENV=production tsx scripts/db/import-seed-production.ts",
    "types:generate": "node scripts/db/generate-types.js test",
    "types:generate:prod": "node scripts/db/generate-types.js prod",
    "//    ": "",
    "db:check-drift": "npx supabase db diff --linked",
    "db:pull-schema": "npx supabase db diff --linked -f schema_update",
    "//     ": "",
    "optimize-images": "tsx scripts/optimize-images.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "auto-parts",
    "cross-reference",
    "search",
    "automotive",
    "b2b"
  ],
  "author": "ACR Automotive",
  "license": "ISC",
  "description": "Auto parts cross-reference search system for ACR Automotive",
